{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "Data Source=myapp.db"
  },
  "GoogleCloudStorage": {
    "BucketName": "your-gcs-bucket-name"
  },
  "GoogleDocumentAI": {
    "ProjectId": "your-gcp-project-id",
    "Location": "your-gcp-location",
    "ProcessorId": "your-document-processor-id"
  },
  "GoogleVertexAI": {
    "ProjectId": "your-gcp-project-id",
    "Location": "us-central1", // Common for Vertex AI
    "Publisher": "google",
    "Model": "gemini-1.5-flash-001", // Or gemini-1.5-pro-001
    "EmbeddingModel": "text-embedding-004" // For text embeddings
  }
}
